package battleships;

public class Battleships 
{

	public static void main(String[] args)
	{
		int[][] twoDArray = {{0,1,2}, {1,2,3}, {2,3,4}};
		
		for(int i = 0; i< twoDArray.length; i++)
		{
			for(int j = 0; j <twoDArray.length; j++)
			{
				System.out.println(twoDArray[i][j]);
			}
		}
		
	}

}
